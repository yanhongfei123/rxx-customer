@import "../../common/vendor.wxss";

@charset "UTF-8";
page {
  margin-bottom: 100rpx;
}
.uni-list-cell {
  width: 690rpx;
  margin: 28rpx auto 0;
  background: #fff;
  box-sizing: border-box;
  font-size: 32rpx;
  color: #1A1A1A;
  overflow: hidden;
}
.order-number {
  font-size: 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  padding-left: 20rpx;
  border-bottom: 2rpx solid #E6E6E6;
}
.cell-mid {
  position: relative;
  padding: 15rpx 0;
}
.cell-mid .state {
  position: absolute;
  top: 19rpx;
  right: 0;
  width: 136rpx;
  height: 56rpx;
  line-height: 56rpx;
  color: #FF9833;
  text-align: center;
  border-radius: 30rpx;
  border: 2rpx solid #FF9833;
}
.h-70 {
  height: 70rpx;
  line-height: 70rpx;
  padding-left: 20rpx;
}
.user-info {
  width: 690rpx;
  margin: 54rpx auto 0;
  text-align: center;
}
.user-info .top-wrap {
  position: relative;
  height: 2rpx;
  background: #DADFE2;
}
.user-info .top-wrap .top-title {
  position: absolute;
  left: 50%;
  top: -18rpx;
  height: 36rpx;
  line-height: 36rpx;
  padding: 0 20rpx;
  background: #fff;
  font-size: 30rpx;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
}
.user-info .head-logo {
  border-radius: 100rpx;
  margin: 60rpx 0 30rpx;
  width: 120rpx;
  height: 120rpx;
}
.user-info .manager {
  margin-bottom: 22rpx;
}
.user-info .praise-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  margin: 60rpx 0;
}
.user-info .praise-wrap .star-cont {
  margin: 0 22rpx;
}
.user-info .praise-wrap .star {
  width: 63rpx;
  height: 61rpx;
}