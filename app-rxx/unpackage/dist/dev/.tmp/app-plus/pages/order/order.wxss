.no-data {
  text-align: center;
  font-size: 30rpx;
  margin-top: 100rpx;
}
page {
  background: #F5F5F5;
  margin-bottom: 100rpx;
}
.uni-list-cell {
  width: 690rpx;
  margin: 28rpx auto 0;
  background: #fff;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  font-size: 32rpx;
  color: #1A1A1A;
  overflow: hidden;
}
.order-number {
  font-size: 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  padding-left: 34rpx;
  border-bottom: 2rpx solid #E6E6E6;
}
.cell-mid {
  position: relative;
  padding: 15rpx 0;
}
.cell-mid .state {
    position: absolute;
    top: 19rpx;
    right: 0;
    width: 160rpx;
    height: 60rpx;
    line-height: 60rpx;
    color: #fff;
    background: #BBB;
    text-align: center;
    border-top-left-radius: 30rpx;
    border-bottom-left-radius: 30rpx;
}
.cell-mid .light {
    background: #FF9833;
}
.h-70 {
  height: 70rpx;
  line-height: 70rpx;
  padding-left: 34rpx;
}
.go-detail {
  font-size: 34rpx;
  color: #FF9833;
  height: 88rpx;
  line-height: 88rpx;
  border-top: 2rpx solid #E6E6E6;
  text-align: center;
}

