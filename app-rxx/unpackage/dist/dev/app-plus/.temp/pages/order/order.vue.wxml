<template name="4be7eea4">
  <view class="_view data-v-2f709302 content">
    <view class="_view data-v-2f709302 uni-list">
      <view hidden="{{!(!orderList.length)}}" class="_view data-v-2f709302 no-data">暂无数据</view>
      <view class="_view data-v-2f709302 uni-list-cell" wx:key="index" key="{{index}}" wx:for="{{orderList}}" wx:for-index="index" wx:for-item="item">
        <view class="_view data-v-2f709302 order-number">订单编号: {{item.order_num}} </view>
        <view class="_view data-v-2f709302 cell-mid">
          <view class="_view data-v-2f709302 manager h-70">贷款经理: {{item.CUSTOMER.user.NAME}} </view>
          <view class="_view data-v-2f709302 apply-money h-70">贷款额度: {{item.amount}} </view>
          <view class="_view data-v-2f709302 apply-name h-70">产品名称: {{item.pruductname}} </view>
          <view class="_view data-v-2f709302 apply-type h-70">贷款类型: {{item.typename}} </view>
          <view class="_view data-v-2f709302 state {{[[item.STATUS === 2 ? 'light' : '']]}}">{{state[item.STATUS - 1]}}</view>
        </view>
        <view class="_view data-v-2f709302 go-detail" bindtap="handleProxy" data-eventid="{{'82c-0-'+index}}" data-comkey="{{$k}}">查看详细</view>
      </view>
    </view>
  </view>
</template>